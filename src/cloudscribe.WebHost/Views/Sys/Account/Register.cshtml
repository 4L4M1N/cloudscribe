@model cloudscribe.Core.Web.ViewModels.Account.EditUserViewModel
@{
    
}
<h2>@ViewBag.Title</h2>
@using (Html.BeginForm("Register", "Account", FormMethod.Post, new { @class = "form-horizontal", role = "form" }))
{
    @Html.AntiForgeryToken()
    <h4>@CommonResources.CreateANewAccount</h4>
    <hr />
    @Html.ValidationSummary("", new { @class = "text-danger" })
    @Html.Partial("EditUserPartial", Model)
    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" class="btn btn-default" value="@CommonResources.Register" />
        </div>
    </div>
}
@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
