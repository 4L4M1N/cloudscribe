<environment names="Development">
    <script src="~/cscsr/jqueryvalidate"></script>
    <script src="~/cscsr/jqueryvalidationunobtrusive"></script>
</environment>
<environment names="Staging,Production">
    <script src="//ajax.aspnetcdn.com/ajax/jquery.validate/1.14.0/jquery.validate.min.js"
               asp-fallback-src="~/cscsr/jqueryvalidate"
               asp-fallback-test="window.jquery && window.jquery.validator">
    </script>
    <script src="//ajax.aspnetcdn.com/ajax/jquery.validation.unobtrusive/3.2.6/jquery.validate.unobtrusive.min.js"
               asp-fallback-src="~/cscsr/jqueryvalidationunobtrusive"
               asp-fallback-test="window.jquery && window.jquery.validator && window.jquery.validator.unobtrusive">
    </script>
</environment>

