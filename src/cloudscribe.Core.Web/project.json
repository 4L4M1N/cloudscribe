{
    "version": "1.0.0-*",
    "description": "Multi-tenant MVC and WebApi application foundation with management for sites, users, roles, and geographic data.  You should only install this into an empty web application project that does not already have startup code or AspNet Identity installed. This is the main library for cloudscribe. Typically you should not install this directly but instead install the package cloudscribe.Core.Web.Integration which will also install cloudscribe.Web.Core along with the integration code needed to wire it up in your empty web application project. You are allowed to modify the integration code after it is added to your project.",

    "authors": [ "Joe Audette" ],
    "packOptions": {
        "owners": [ "Source Tree Solutions" ],
        "tags": [ "cloudscribe", "multi-tenant", "mvc6", "site management" ],
        "projectUrl": "https://github.com/joeaudette/cloudscribe",
        "licenseUrl": "http://www.apache.org/licenses/LICENSE-2.0",
        "copyright": "2015 Source Tree Solutions",
        "iconUrl": "https://raw.githubusercontent.com/joeaudette/cloudscribe/master/cloudscribe-icon-32.png",
        "requireLicenseAcceptance": false,
        "summary": "Multi-tenant MVC and WebApi application foundation with management for sites, users, roles, and geographic data",
        "releaseNotes": ""
    },

    "buildOptions": {
