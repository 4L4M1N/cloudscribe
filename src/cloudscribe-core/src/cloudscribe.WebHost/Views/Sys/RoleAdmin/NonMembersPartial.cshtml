@model cloudscribe.Core.Web.ViewModels.RoleAdmin.RoleMemberListViewModel
@{
    
}
<div class="modal-dialog modal-lg">
    <div class="modal-content">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-label="@CommonResources.Close"><span aria-hidden="true">&times;</span></button>
            <h2 class="modal-title">@Model.Role.DisplayName</h2>
        </div>
        <div class="modal-body">
            <h3>@Model.Heading</h3>
            
            <div id="nonmembersgridcontainer">
                @Html.Partial("NonMembersGridPartial", Model)
            </div>
        </div>
        <div class="modal-footer">
            <button type="button" class="btn btn-default" data-dismiss="modal">@CommonResources.Close</button>
        </div>
    </div>
</div>
